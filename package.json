{
  "name": "@robojones/nginx-log-parser",
  "main": "build/Parser",
  "typings": "build/Parser",
  "description": "Parse Nginx logs with this simple parser.",
  "version": "0.0.5",
  "scripts": {
    "watch": "tsc --watch",
    "lint": "tslint --project .",
    "build": "tsc",
    "test": "yarn build && yarn nyc",
    "nyc": "nyc mocha 'build/test/**/*.js'"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robojones/nginx-log-parser.git"
  },
  "keywords": [
    "nginx",
    "logs",
    "parse",
    "parser"
  ],
  "author": "robojones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robojones/nginx-log-parser/issues"
  },
  "homepage": "https://github.com/robojones/nginx-log-parser#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "tslint": "^5.11.0",
    "tslint-config-ultra": "^2.0.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/node": "^10.9.4"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ],
    "exclude": [
      "build/test"
    ]
  }
}
